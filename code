UICorner6.Parent = ExecuteButton

-- Function to show a simple Roblox notification
local function showSimpleNotification(title, message)
    StarterGui:SetCore("SendNotification", {
        Title = title,
        Text = message,
        Duration = 4 -- Duration in seconds
    })
end

ExecuteButton.MouseButton1Click:Connect(function()
    local scriptContent = ScriptEditor.Text
    if scriptContent ~= "" then
        local success, err = pcall(function()
            loadstring(scriptContent)()
        end)
        if success then
            showSimpleNotification(NOTIFICATION_TITLE, "Script executed successfully!\nRemember to use scripts safely!")
        else
            showSimpleNotification(NOTIFICATION_TITLE, "Execution Error: " .. tostring(err) .. "\nUse scripts safely!")
        end
    end
end)

