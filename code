-- Configuration for the Notification System
local NOTIFICATION_TITLE = "Ronix Executor"
local NOTIFICATION_MESSAGE = "Quantim force(UNC 99%) PLEASE PRESS ] TO OPEN."

-- Services
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local StarterGui = game:GetService("StarterGui") -- Added StarterGui service for built-in notifications

-- Create ScreenGui
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "RonixExecutorGUI"
ScreenGui.Parent = LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ResetOnSpawn = false
ScreenGui.Enabled = false -- Start disabled

-- Main Frame
local MainFrame = Instance.new("Frame")
MainFrame.Size = UDim2.new(0, 700, 0, 400) -- Increased width to fit the loadstrings panel
MainFrame.Position = UDim2.new(0.5, -350, 0.5, -200) -- Re-center the wider frame
MainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
MainFrame.BorderColor3 = Color3.fromRGB(20, 20, 20)
MainFrame.BorderSizePixel = 2
MainFrame.Draggable = false -- Set to false as we're handling custom dragging
MainFrame.Parent = ScreenGui
MainFrame.Visible = false -- Start invisible for animation

-- UI Corner for MainFrame
local UICorner = Instance.new("UICorner")
UICorner.CornerRadius = UDim.new(0, 8)
UICorner.Parent = MainFrame

-- Top Bar
local TopBar = Instance.new("Frame")
TopBar.Size = UDim2.new(1, 0, 0, 30)
TopBar.Position = UDim2.new(0, 0, 0, 0)
TopBar.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
TopBar.BorderColor3 = Color3.fromRGB(20, 20, 20)
TopBar.BorderSizePixel = 1
TopBar.Parent = MainFrame

-- UI Corner for TopBar
local UICorner2 = Instance.new("UICorner")
UICorner2.CornerRadius = UDim.new(0, 8)
UICorner2.Parent = TopBar

-- Title Label
local TitleLabel = Instance.new("TextLabel")
TitleLabel.Size = UDim2.new(1, -60, 1, 0)
TitleLabel.Position = UDim2.new(0, 0, 0, 0)
TitleLabel.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
TitleLabel.BackgroundTransparency = 1
TitleLabel.Font = Enum.Font.SourceSansBold
TitleLabel.TextSize = 18
TitleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TitleLabel.Text = "Quantim Force (99% UNC)"
TitleLabel.TextXAlignment = Enum.TextXAlignment.Center
TitleLabel.Parent = TopBar

-- Close Button
local CloseButton = Instance.new("TextButton")
CloseButton.Size = UDim2.new(0, 25, 0, 25)
CloseButton.Position = UDim2.new(1, -30, 0.5, -12.5)
CloseButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
CloseButton.BorderColor3 = Color3.fromRGB(150, 0, 0)
CloseButton.BorderSizePixel = 1
CloseButton.Font = Enum.Font.SourceSansBold
CloseButton.TextSize = 18
CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255)
CloseButton.Text = "X"
CloseButton.Parent = TopBar

-- UI Corner for CloseButton
local UICorner3 = Instance.new("UICorner")
UICorner3.CornerRadius = UDim.new(0, 4)
UICorner3.Parent = CloseButton

CloseButton.MouseButton1Click:Connect(function()
    -- Tween the GUI off-screen and then destroy it
    local closeTweenInfo = TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
    local closeTween = TweenService:Create(MainFrame, closeTweenInfo, {Position = UDim2.new(0.5, -350, 1.5, -200), BackgroundTransparency = 1, BorderSizePixel = 0})
    closeTween:Play()
    closeTween.Completed:Connect(function()
        ScreenGui:Destroy() -- Destroy the entire ScreenGui
    end)
end)

-- Minimize Button (optional, but good for an executor)
