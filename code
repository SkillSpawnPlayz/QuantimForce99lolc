-- Removed collapsing functionality logic

-- Function to create a loadstring button
local function createLoadstringButton(name, loadstringCode)
    local ButtonFrame = Instance.new("Frame")
    ButtonFrame.Size = UDim2.new(1, -20, 0, 50)
    ButtonFrame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    ButtonFrame.BorderColor3 = Color3.fromRGB(30, 30, 30)
    ButtonFrame.BorderSizePixel = 1
    ButtonFrame.Parent = LoadstringsContentFrame

    local UICornerBtn = Instance.new("UICorner")
    UICornerBtn.CornerRadius = UDim.new(0, 6)
    UICornerBtn.Parent = ButtonFrame

    local LoadstringButton = Instance.new("TextButton")
    LoadstringButton.Size = UDim2.new(1, 0, 0.6, 0)
    LoadstringButton.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    LoadstringButton.BorderColor3 = Color3.fromRGB(30, 30, 30)
    LoadstringButton.BorderSizePixel = 0
    LoadstringButton.Font = Enum.Font.SourceSansBold
    LoadstringButton.TextSize = 14
    LoadstringButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    LoadstringButton.Text = name
    LoadstringButton.Parent = ButtonFrame

    local KeylessLabel = Instance.new("TextLabel")
    KeylessLabel.Size = UDim2.new(1, 0, 0.4, 0)
    KeylessLabel.Position = UDim2.new(0, 0, 0.6, 0)
    KeylessLabel.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    KeylessLabel.BackgroundTransparency = 1
    KeylessLabel.Font = Enum.Font.SourceSans
    KeylessLabel.TextSize = 12
    KeylessLabel.TextColor3 = Color3.fromRGB(180, 180, 180)
    KeylessLabel.Text = "(keyless)"
    KeylessLabel.Parent = ButtonFrame

    LoadstringButton.MouseButton1Click:Connect(function()
        -- Only populate the ScriptEditor.Text, do not execute here.
        ScriptEditor.Text = loadstringCode
        showSimpleNotification(NOTIFICATION_TITLE, "Loaded: " .. name .. " into editor.\nPress 'Execute' to run.")
    end)

    -- Update CanvasSize after adding each button
    -- This ensures the scrolling frame adjusts to the content.
    -- task.wait() is used to allow the UIListLayout to calculate its size first.
    task.wait(0.05) -- Small delay to allow layout to update
    LoadstringsContentFrame.CanvasSize = UDim2.new(0, 0, 0, UIListLayout.AbsoluteContentSize.Y)
end

-- Loadstring Buttons
